{
  "name": "gyratify",
  "version": "1.0.0",
  "description": "Gyratify",
  "repository": "git@github.com:MaxRozh/gyratife.git",
  "license": "MIT",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "start": "nodemon -w server server/cluster.js",
    "start:dev": "nodemon -w server server/index.js",
    "prod:analyze": "ANALYZE=true yarn build:prod",
    "lint:js": "eslint 'src/**/*.{js,jsx}' 'server/**/*.{js,jsx}' --quiet --cache",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint": "yarn lint:js && yarn lint:css",
    "test": "jest",
    "test:related": "jest -o --passWithNoTests",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@keyv/redis": "^2.1.2",
    "@sentry/browser": "^5.26.0",
    "@sentry/node": "^5.26.0",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "immer": "^7.0.9",
    "isomorphic-fetch": "^3.0.0",
    "keyv": "^4.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-lazyload": "^3.0.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-saga": "1.1.3",
    "reselect": "^4.0.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/helper-call-delegate": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-do-expressions": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/polyfill": "^7.11.5",
    "@sentry/integrations": "^5.26.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "compression": "^1.7.4",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-redux-saga": "^1.2.1",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "lint-staged": "^10.4.0",
    "nodemon": "^2.0.4",
    "pm2": "^4.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "stylelint": "13.7.2",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-manifest-plugin": "^2.2.0",
    "winston": "^3.3.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn prettier --write 'src/**/*.{js,jsx}' 'server/**/*.{js,jsx}'",
      "yarn lint:js",
      "git add"
    ],
    "src/**/*.css": [
      "yarn prettier --write 'src/**/*.css' 'apps/**/*.css'",
      "yarn lint:css",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test:related"
    }
  }
}
